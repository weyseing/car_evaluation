1745056800325:cramers_v <- function(tbl) {
1745056800326:cv <- sqrt(chisq.test(tbl)$statistic / (sum(tbl) * min(dim(tbl) - 1)))
1745056800326:return(as.numeric(cv))
1745056800326:}
