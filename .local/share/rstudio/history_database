1745055524432:as.numeric(car_data$class),
1745055524432:method = "spearman")
1745055524432:data.frame(Feature = feat,
1745055524433:Spearman_Rho = test$estimate,
1745055524433:P_Value = test$p.value)
1745055524433:})
